{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/check-availability",
            "dest": "/api/check-availability.js"
        },
        {
            "src": "/api/reserve",
            "dest": "/api/reserve.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}